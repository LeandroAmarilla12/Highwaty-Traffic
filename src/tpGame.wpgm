import wollok.game.*
import Fondo.*

program SpyHunter {
	
//	CONFIG	
	game.title("TP")
	game.height(36)
	game.width(30)
	game.ground("mapita.png")
	game.cellSize(10)
	
// FONDO
	game.addVisual(pista)
	game.onTick(500, "pista moviendose", { => pista.mover() })

	
// MOVERSE
	game.addVisualCharacter(auto)
	
//	START
	game.start()
}